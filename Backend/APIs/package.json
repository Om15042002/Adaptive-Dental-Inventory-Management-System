{
  "name": "adaptive-dental-inventory-api",
  "version": "1.0.0",
  "description": "Intelligent inventory management system for dental practices with predictive analytics",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate:phase1": "node scripts/validate-phase1.js",
    "db:init": "mysql -u root -p dental_inventory < phase1_schema.sql"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.15.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "dental",
    "inventory",
    "management",
    "api",
    "healthcare"
  ]
}
